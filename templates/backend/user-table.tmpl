{{define "user-table"}}
<table class="table table-hover">
	<thead>
		<tr>
			<th class="col-md-4">first name</th>
			<th class="col-md-4">last name</th>
			<th class="col-md-4">mail address</th>
		</tr>
	</thead>
	<tbody>
		{{range .}}
		<tr>
			<td>{{.FirstName}}</td>
			<td>{{.LastName}}</td>
			<td>{{.MailAddress}}</td>
		</tr>
		{{end}}
	</tbody>
</table>
{{end}}
